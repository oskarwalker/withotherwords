{
  "name": "withotherwords",
  "version": "1.0.0",
  "description": "A hybridapp of the game With other words",
  "main": "index.js",
  "scripts": {
    "start": "HOST='localhost' node --harmony ./server/index.js",
    "start:production": "NODE_ENV=production HOST='localhost' node --harmony ./server/index.js",
    "js:dev-server": "node ./webpack-dev-server.js",
    "build:js": "webpack --config ./webpack.development.config.js",
    "build:js:production": "NODE_ENV=production webpack --config ./webpack.production.config.js",
    "build:css": "cssnano client/src/css/style.css public/css/style.css",
    "build:all": "yarn run build:css && yarn run build:js",
    "build:all:production": "yarn run build:css && yarn run build:js:production",
    "build:ios": "yarn run build:all:production && node scripts/prepare-cordova-ios.js",
    "lint": "standard --verbose \"server/**/*.js\" \"client/**/*.jsx\" \"client/js/**/*.js\" | snazzy || true",
    "lint:fix": "standard --fix \"server/**/*.js\" \"client/**/*.jsx\" \"client/js/**/*.js\" || true"
  },
  "author": "Oskar Walker, David Dokakis & Daniel Hidesten",
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.22.0",
    "body-parser": "1.16.0",
    "cookie": "0.3.1",
    "cookie-parser": "1.4.3",
    "cssnano-cli": "^1.0.5",
    "express": "4.14.0",
    "helmet": "3.4.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-fastclick": "^2.1.2",
    "react-timer-hoc": "^2.0.2",
    "rethinkdb": "2.3.3",
    "socket.io": "1.7.2",
    "timesync-socket": "^1.0.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "shelljs": "^0.7.6",
    "snazzy": "6.0.0",
    "standard": "8.6.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
